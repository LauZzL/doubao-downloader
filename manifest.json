{
  "manifest_version": 3,
  "name": "豆包下载器",
  "version": "1.2.4",
  "description": "开源、免费、高效的浏览器扩展，支持一键批量去水印下载豆包AI生成的图片。",
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://www.doubao.com/*"],
      "js": ["doubao-downloader.user.js"],
      "run_at": "document_end",
      "world": "MAIN"
    }
  ],
  "permissions": ["activeTab"]
}
